        android:id="@+id/col4"
        android:layout_width="@dimen/grid_thickness"
        android:layout_height="match_parent"
        app:layout_marginLeftPercent="40%"
        android:background="@drawable/dashed_vert"
        />

    <View
        android:id="@+id/col5"
        android:layout_width="@dimen/grid_thickness"
        android:layout_height="match_parent"
        app:layout_marginLeftPercent="50%"
        android:background="@drawable/dashed_vert"
        />

    <View
        android:id="@+id/col6"
